{
  "version": 2,
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)\\.(html)$",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    },
    {
      "source": "/",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    },
    {
      "source": "/setup",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    },
    {
      "source": "/setup/",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    },
    {
      "source": "/api/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    }
  ]
}

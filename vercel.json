{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)\\.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/widget/:id", "destination": "/widget/index.html?id=:id" },
    { "source": "/setup", "destination": "/setup/index.html" },
    { "source": "/widget", "destination": "/widget/index.html" }
  ]
}

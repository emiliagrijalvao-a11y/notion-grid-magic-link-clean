{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/setup/?$", "dest": "/setup/index.html" },
    { "src": "^/help/?$", "dest": "/help/index.html" },
    { "src": "^/account/?$", "dest": "/account/index.html" },
    { "src": "^/widget/?$", "dest": "/widget/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, max-age=0" }
      ]
    },
    {
      "source": "/setup/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, max-age=0" }
      ]
    }
  ]
}
